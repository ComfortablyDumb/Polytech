def Crypto(string,key):
    alpha = "abcdefghijklmnopqrstuvwxyz"
    listAlpha = list(alpha)
    listWordsKey = key.split(" ")
    listKey = list(key)
    for char in key:
        listAlpha.remove(char)
    new_alpha = "".join(listAlpha)
    new_key = listWordsKey[0] + new_alpha
    return new_key

print(Crypto("coucou","coucou"))
    

